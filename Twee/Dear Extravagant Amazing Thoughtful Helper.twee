:: StoryStylesheet [stylesheet]
@import url('https://fonts.googleapis.com/css2?family=Press+Start+2P&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Comic+Neue:ital,wght@0,300;0,400;0,700;1,300;1,400;1,700&display=swap');

*{
  word-break: break-word;
}
audio::-webkit-media-controls {
  display: none;
}
tw-story {
  background-size:cover;
  background-repeat: no-repeat;
  background-position: center;
  box-shadow: inline 0 0 5em 1em #0000;
}
tw-story[tags = "JumpScare"]{
   background-color:black;
}
tw-passage[tags = "JumpScare"]{
  position: absolute;
  z-index:0;
  animation: loadIn 1s linear;
  color: red;
  height:100%;
  width:100%;
  left:0;
  top:0;
}
.cantEscapeText{
  color:red;
  animation: loadIn 11s linear;
}
.jumpScareClass{
  position:fixed;
  z-index: 999;
  opacity: 0;
  background-image: url("Media/Sad_Glob.png");
  background-repeat: no-repeat;
  background-size:cover;
  background-position: center;
  height:100%;
  width: 100%;
  top:0;
  left:0;
 
}
tw-story[tags = "Intermission"]{
background-image: url("Media/Background2Edited.png")
}
tw-passage[tags ="GameOver"]{
  position:absolute;
  background-color: white;
  animation: loadIn 2s linear;
  left:0;
  top:0;
  height:100%;
  width:100%;
  background-image: url("Media/GAME_OVER_screen.png");
}
tw-story[tags = "Outside"]{
  background-image: url("Media/Background1Edited.png");
  background-color: black;
  background-repeat: no-repeat;
}
tw-story[tags ="Web"]{
  position:absolute;
  left:0;
  top:0;
  margin:0;
  padding:0;
}
tw-passage[tags = "Web"]{
  position:absolute;
  margin:0;
  left:0;
  top:0;
  padding: 0px;
  width: 100%;
  height:100%;
  min-height:500px;
  row-gap: 10% 10% 10% 0 0;
  font-family: "Press Start 2P", system-ui;
  font-weight: 400;
  font-style: normal;
  overflow: auto;
  background-color: white;
  display: grid;
  animation: loadIn 2s linear;
  grid-template-rows: 22% 90% 40% 0 0;
  grid-template-columns: 5% 5% 10% 5% 55% 5% 5% 10%;
  grid-template-areas:
    'header header header header header header header header'
    'sidebarLeft sidebarLeft sidebarLeft main main sidebarRight sidebarRight sidebarRight'
    '0 bottomLeft bottomLeft bottomLeft bottomMain bottomMain bottomMain bottomMain';
}
@keyframes loadIn{
  0% {opacity:0}
  50% {opacity:0}
  100% {opacity:1}
}
@keyframes zoomIn{
  0% {height: 10%;}
  99% {height:30%;}
  100% {height:100%;}
}
tw-passage[tags ="Web"] > div {
  text-align:center;
  color:black;
  font-size:20px;
  border: 2px solid black;
}

tw-passage[tags ="Web"] .ButtonChoices{
  position: relative;
}

.gridHeader{
  grid-area: header;
  background-size: cover;
  background-position:right;
  background-image: url("Media/HeaderImageForWeb.png");
}

.gridSidebarLeft{
  grid-area: sidebarLeft;
  margin-top:7px;
  margin-left:7px;
  margin-bottom:7px;
}
.gridSidebarLeftGlobDiv{
  height:50%;
  width:100%;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("Media/SideBarLeft1.png");
}
.gridSidebarLeftGlobDiv img{
  height:100%;
}
.gridSidebarLeftImageDiv2{
  height:50%;
  width:100%;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("Media/SideBarLeft2.png");
}
.gridMain{ 
  grid-area: main;
  word-break: break-word;
  overflow: auto;
  padding:5px;
  margin:7px;
}
.gridMain p {
 font-size: calc(13px +2vmin);
  
}
.gridSidebarRight{
  margin-top:7px;
  margin-right:7px;
  margin-bottom:7px;
  grid-area: sidebarRight;
  background-size:contain;
  background-position:center;
  background-repeat: no-repeat;
  
}
.gridSidebarRightImageDiv1{
  height:50%;
  width:100%;
  background-position:center;
  background-repeat: no-repeat;
  background-image: url("Media/SideBarRight1.png");
  
}
.gridSidebarRightImageDiv2{
  height:50%;
  width:100%;
  background-size:contain;
  background-position:center;
  background-repeat: no-repeat;
  background-image: url("Media/SideBarRight2.png");
  
}
.gridBottomLeft{
  grid-area: bottomLeft;
  overflow-y: scroll;
  margin: 15px;
}
.gridBottomLeftHeaderBox{
  margin:0;
  padding:0;
  width:100%;
}
.gridBottomLeftTextBox{
  color: black;
  padding-top:15px;
  font-size: calc(10px + 0.7vmin);
  position: relative;
}
.gridBottomLeftHeaderBox > h1 {
  text-align: center;
  color: white;
  font-size: calc(20px + 3.3vmin);
  -webkit-text-stroke-width: 2px;
  font-family: "Comic Neue", cursive;
  font-weight: 400;
  font-style: normal;
}
.gridBottomLeftHeaderBox > h1::after {
  content: '';
  position:absolute;
  left: 15%;
  bottom:-15px;
  width: 75%; 
  height: 2px; 
  background-color: black; 
}
.gridBottomMain{
  grid-area: bottomMain;
  margin-right:7px;
  margin-top: 15px;
}
.gridBottomMainHeaderBox {
  padding-right: 15px;
}
.gridBottomMainHeaderBox > h1 {
  text-align: right;
  z-index:1;
  color: white;
  font-size: calc(20px + 3.3vmin);
  -webkit-text-stroke-width: 2px;
  font-family: "Comic Neue", cursive;
  font-weight: 400;
  font-style: normal;
}
.gridBottomMainImageBox {
  position: absolute;
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
  top:0;
  left:0;
  height:100%;
  width:100%;
}
.gridBottomMainImageBox img{
  bottom:0;
  margin-right:10%;
  margin-left:4%;
  width: 34%;
}


.rgb-bg1 {
  background-color: red;
  animation: rgb-bg 2.5s linear;
  animation-iteration-count: infinite;
}
.rgb1 {
  animation: rgbAnim 2.5s linear;
  animation-iteration-count: infinite;
}

@keyframes rgb-bg {
  100%,0%{
      background-color: rgb(255,0,0);
  }
  8%{
      background-color: rgb(255,127,0);
  }
  16%{
      background-color: rgb(255,255,0);
  }
  25%{
      background-color: rgb(127,255,0);
  }
  33%{
      background-color: rgb(0,255,0);
  }
  41%{
      background-color: rgb(0,255,127);
  }
  50%{
      background-color: rgb(0,255,255);
  }
  58%{
      background-color: rgb(0,127,255);
  }
  66%{
      background-color: rgb(0,0,255);
  }
  75%{
      background-color: rgb(127,0,255);
  }
  83%{
      background-color: rgb(255,0,255);
  }
  91%{
      background-color: rgb(255,0,127);
  }
}

@keyframes rgbAnim{
  100%,0%{
    color: rgb(255,0,0);
  }
  8%{
    color: rgb(255,127,0);
  }
  16%{
    color: rgb(255,255,0);
  }
  25%{
    color: rgb(127,255,0);
  }
  33%{
    color: rgb(0,255,0);
  }
  41%{
    color: rgb(0,255,127);
  }
  50%{
    color: rgb(0,255,255);
  }
  58%{
    color: rgb(0,127,255);
  }
  66%{
    color: rgb(0,0,255);
  }
  75%{
    color: rgb(127,0,255);
  }
  83%{
    color: rgb(255,0,255);
  }
  91%{
    color: rgb(255,0,127);
  }
}

.ButtonChoices {
  display: flex;
  flex-wrap: nowrap;
  position: fixed;
  bottom:0;
  right:0;
  width: 100%;
 	
}

.ButtonChoices tw-expression {
  width:100%;
}

.ButtonChoices tw-expression:not(:last-child){
  margin-right:10px;
}

.ButtonChoices div {
  background-color: rgb(0,0,0);
  color: white;
  width: 100%;
  z-index: 100;
  text-align: center;
  font-size: 20px;
  border: 5px solid white;
}
.ButtonChoices div:hover {
  background-color: rgb(10,10,10);
}
.ButtonChoices div p {
  user-select: none;
}
#ChangeChildToYes:hover p{visibility:hidden;}
#ChangeChildToYes:hover p:after{content: "Yes"; visibility: visible; color: red; position:absolute; left: 43%;}
#ChangeChildToFriend:hover p{visibility:hidden;}
#ChangeChildToFriend:hover p:after{content: "Friend"; visibility: visible; color: red; position:absolute; left: 38%;}
