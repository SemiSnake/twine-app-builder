*{
  word-break: break-word;
}

tw-story {
  background-size:cover;
  background-repeat: no-repeat;
  background-position: center;
  box-shadow: inline 0 0 5em 1em #0000;
 
}

tw-story[tags = "Outside"]{
  background-image: url("Media/Background1.png");
}
tw-story[tags ="Web"]{
  position:absolute;
  left:0;
  top:0;
  margin:0;
  padding:0;
}
tw-passage[tags = "Web"]{
  position:absolute;
  margin:0;
  left:0;
  top:0;
  padding: 0px;
  width: 100%;
  height:100%;
  min-height:500px;
  row-gap: 10% 10% 10% 0 0;
  font-family: "Press Start 2P", system-ui;
  font-weight: 400;
  font-style: normal;
  overflow: auto;
  background-color: white;
  display: grid;
  grid-template-rows: 22% 90% 40% 0 0;
  grid-template-columns: 5% 5% 10% 5% 55% 5% 5% 10%;
  grid-template-areas:
    'header header header header header header header header'
    'sidebarLeft sidebarLeft sidebarLeft main main sidebarRight sidebarRight sidebarRight'
    '0 bottomLeft bottomLeft bottomLeft bottomMain bottomMain bottomMain bottomMain';
}
@keyframes transition_1 {
  from {opacity:0;}
  to {opacity:1;}
}
tw-passage[tags ="Web"] > div {
  text-align:center;
  color:black;
  font-size:20px;
  border: 2px solid black;
}
tw-passage[tags ="Web"] .ButtonChoices{
  position: relative;
}
.gridHeader{
  grid-area: header;
  background-size: cover;
  background-position:right;
  background-image: url("https://cdn.discordapp.com/attachments/652181458928271371/1287681706479976448/image.png?ex=66f26e8a&is=66f11d0a&hm=a612f96cb6f84373b0d72d2496929f8a2b0b8ccb144933e2dc6d2349d97dbb1d&");
  
}
.gridSidebarLeft{
  grid-area: sidebarLeft;
  margin-top:7px;
  margin-left:7px;
  margin-bottom:7px;
  background-size:contain;
  background-position:center;
  background-repeat: no-repeat;
  background-image: url("https://cdn.discordapp.com/attachments/652181458928271371/1287683159600664628/image.png?ex=66f26fe5&is=66f11e65&hm=1affb0b4a82010ec4f4c4d7b7d037fd4f295f92124419d9afbfbae79cf9dad00&")
}
.gridMain{ 
  grid-area: main;
  word-break: break-word;
  overflow: auto;
  padding:5px;
  margin:7px;
}
.gridMain p {
 font-size: calc(13px +2vmin);
  
}
.gridSidebarRight{
  margin-top:7px;
  margin-right:7px;
  margin-bottom:7px;
  grid-area: sidebarRight;
  background-size:contain;
  background-position:center;
  background-repeat: no-repeat;
  background-image: url("https://cdn.discordapp.com/attachments/652181458928271371/1287684980859732039/image.png?ex=66f27197&is=66f12017&hm=1e2b1adb81065e5181b822ac42597a1bd0ac9f168aaeee0ed1762d4ea0d62011&");
  
}
.gridBottomLeft{
  grid-area: bottomLeft;
  overflow-y: scroll;
  margin: 15px;
}
.gridBottomLeftHeaderBox{
  margin:0;
  padding:0;
  width:100%;
}
.gridBottomLeftTextBox{
  color: black;
  padding-top:15px;
  font-size: calc(10px + 0.7vmin);
  position: relative;
}
.gridBottomLeftHeaderBox > h1 {
  text-align: center;
  color: white;
  font-size: calc(20px + 3.3vmin);
  -webkit-text-stroke-width: 2px;
  font-family: "Comic Neue", cursive;
  font-weight: 400;
  font-style: normal;
}
.gridBottomLeftHeaderBox > h1::after {
  content: '';
  position:absolute;
  left: 15%;
  bottom:-15px;
  width: 75%; 
  height: 2px; 
  background-color: black; 
}
.gridBottomMain{
  grid-area: bottomMain;
  margin-right:7px;
  margin-top: 15px;
}
.gridBottomMainHeaderBox {
  padding-right: 15px;
}
.gridBottomMainHeaderBox > h1 {
  text-align: right;
  z-index:1;
  color: white;
  font-size: calc(20px + 3.3vmin);
  -webkit-text-stroke-width: 2px;
  font-family: "Comic Neue", cursive;
  font-weight: 400;
  font-style: normal;
}
.gridBottomMainImageBox {
  position: absolute;
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
  top:0;
  left:0;
  height:100%;
  width:100%;
}
.gridBottomMainImageBox img{
  bottom:0;
  margin-right:10%;
  margin-left:4%;
  width: 34%;
}


.rgb-bg1 {
  background-color: red;
  animation: rgb-bg 2.5s linear;
  animation-iteration-count: infinite;
}
